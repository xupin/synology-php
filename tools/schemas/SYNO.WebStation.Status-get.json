{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "properties": {
    "VirtualHost": {
      "type": "object",
      "properties": {
        "error": {
          "type": "integer"
        },
        "total": {
          "type": "integer"
        }
      },
      "required": [
        "error",
        "total"
      ]
    },
    "available_php_backend": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    },
    "available_python_backend": {
      "type": "null"
    },
    "available_server_backend": {
      "type": "array",
      "items": {
        "type": "integer"
      }
    },
    "backend": {
      "type": "integer"
    },
    "home_share_status": {
      "type": "integer"
    },
    "php": {
      "type": "string"
    },
    "php_profiles": {
      "type": "object",
      "properties": {
        "profiles": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "backend": {
                "type": "integer"
              },
              "custom_open_basedir": {
                "type": "boolean"
              },
              "default_profile": {
                "type": "null"
              },
              "display_errors": {
                "type": "boolean"
              },
              "enable": {
                "type": "boolean"
              },
              "enable_cache": {
                "type": "boolean"
              },
              "enable_xdebug": {
                "type": "boolean"
              },
              "extensions": {
                "type": "array",
                "items": {
                  "type": "string"
                }
              },
              "fpm_settings": {
                "type": "object",
                "properties": {
                  "max_children": {
                    "type": "integer"
                  },
                  "max_spare_servers": {
                    "type": "integer"
                  },
                  "min_spare_servers": {
                    "type": "integer"
                  },
                  "mode": {
                    "type": "string"
                  },
                  "start_servers": {
                    "type": "integer"
                  }
                },
                "required": [
                  "max_children",
                  "max_spare_servers",
                  "min_spare_servers",
                  "mode",
                  "start_servers"
                ]
              },
              "group": {
                "type": "string"
              },
              "open_basedir": {
                "type": "string"
              },
              "php_settings": {
                "type": "object",
                "properties": {
                  "assert.exception": {
                    "type": "string"
                  },
                  "mysqli.default_socket": {
                    "type": "string"
                  }
                },
                "required": [
                  "assert.exception",
                  "mysqli.default_socket"
                ]
              },
              "profile_desc": {
                "type": "string"
              },
              "profile_name": {
                "type": "string"
              },
              "user": {
                "type": "string"
              },
              "uuid": {
                "type": "string"
              }
            },
            "required": [
              "backend",
              "custom_open_basedir",
              "default_profile",
              "display_errors",
              "enable",
              "enable_cache",
              "enable_xdebug",
              "extensions",
              "fpm_settings",
              "group",
              "open_basedir",
              "php_settings",
              "profile_desc",
              "profile_name",
              "user",
              "uuid"
            ]
          }
        },
        "total": {
          "type": "integer"
        },
        "version": {
          "type": "integer"
        }
      },
      "required": [
        "profiles",
        "total",
        "version"
      ]
    },
    "python_profiles": {
      "type": "object",
      "properties": {
        "profiles": {
          "type": "array"
        },
        "total": {
          "type": "integer"
        },
        "version": {
          "type": "integer"
        }
      },
      "required": [
        "profiles",
        "total",
        "version"
      ]
    },
    "userdir": {
      "type": "boolean"
    },
    "userdir_backend": {
      "type": "integer"
    },
    "userdir_php": {
      "type": "null"
    }
  },
  "required": [
    "VirtualHost",
    "available_php_backend",
    "available_python_backend",
    "available_server_backend",
    "backend",
    "home_share_status",
    "php",
    "php_profiles",
    "python_profiles",
    "userdir",
    "userdir_backend",
    "userdir_php"
  ]
}
